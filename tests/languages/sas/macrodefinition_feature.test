%macro prnt(var,sum);
	proc print data=srhigh;
		var &var;
		sum &sum;
	run;
%mend prnt;

%macro printz/parmbuff;
	%let num=1;
	%let dsname=%scan(&syspbuff,&num);
	%do %while(&dsname ne);
		proc print data=&dsname;
		run;
		%let num=%eval(&num+1);
		%let dsname=%scan(&syspbuff,&num);
	%end;
%mend printz;

