@base <http://example.org/> .

GRAPH <urn:newGraph> {
	<urn:myClass> a rdfs:Class, <other-class> ;
	= <urn:equivalentClass> .
	<urn:myList> a rdf:List ;
		rdf:value [ a <blankNode> ;
			rdf:value ( <list> <of> <things> () [] )
			] ;
		.
}

