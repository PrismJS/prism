""
"Foo
\"bar\"
baz"
''
'Foo
\'bar\'
baz'
'$bar ${4+2}'
'The sum of $x and 3 is ${x + 3}'

----------------------------------------------------

[
	["string", "\"\""],
	["string", "\"Foo\r\n\\\"bar\\\"\r\nbaz\""],
	["string-interpolation", [
		["string", "''"]
	]],
	["string-interpolation", [
		["string", "'Foo\r\n\\'bar\\'\r\nbaz'"]
	]],
	["string-interpolation", [
		["string", "'"],
