			["expression", ["bar"]]
		]],
		["string", " "],
		["interpolation", [
			["interpolation-punctuation", "${"],
			["expression", [
				["number", "4"],
				["operator", "+"],
				["number", "2"]
			]],
			["interpolation-punctuation", "}"]
		]],
		["string", "'"]
	]],
	["string-interpolation", [
		["string", "'The sum of "],
